{
  "name": "copy-paste-path",
  "displayName": "Copy Paste Path",
  "description": "Copy the path to the current file to the clipboard and paste it in the current editor in Unix format (with forward slashes).",
  "version": "0.0.3",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "FabrizioNastri",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabrizionastri/Copy-Paste-Path"
  },
  "license": "MIT",
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.copyPath",
        "title": "Copy Path"
      },
      {
        "command": "extension.copyPastePath",
        "title": "Copy Paste Path"
      },
      {
        "command": "extension.copyRelativePath",
        "title": "Copy Relative Path"
      },
      {
        "command": "extension.copyPasteRelativePath",
        "title": "Copy Paste Relative Path"
      }
    ]
  }
}